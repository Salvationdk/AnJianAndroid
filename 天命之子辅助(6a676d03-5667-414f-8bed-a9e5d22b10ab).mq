/***********自动DS子程序*************/

Function AutoDSAssist
    ReleaseCapture 
	
    For 15
        CvtStr = StrOffset("442|1540|00AEF3-101010,469|1556|005BF6-101010,470|1585|9ACBFF-101010,493|1612|007BFF-101010,658|1775|000A5F-101010", 0 ,Yoffset)
        If CmpColorEx(CvtStr, 0.9) = 1 Then 
            Delay DSDelayTap
            Tap 538, 1649 + Yoffset
            AutoDSAssist = True
            Exit Function
        End If
	
        Delay 100
    Next
	
    AutoDSAssist = False
	
End Function





